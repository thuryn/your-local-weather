<resources xmlns:tools="http://schemas.android.com/tools">
    <string name="app_name" tools:ignore="MissingTranslation">আপনার স্থানীয় আবহাওয়া</string>
    <string name="enter_city_hint" tools:ignore="MissingTranslation">শহরের নাম</string>
    <string name="label_current_city" tools:ignore="MissingTranslation">বর্তমান শহর</string>
    <string name="label_choose_city" tools:ignore="MissingTranslation">শহর নির্বাচন করুন</string>
    <string name="alertDialog_battery_optimization_title" tools:ignore="MissingTranslation">ব্যাটারি অপ্টিমাইজেশন</string>
    <string name="alertDialog_battery_optimization_message" tools:ignore="MissingTranslation">অ্যাপ্লিকেশনটি ব্যাকগ্রাউন্ডে ত্বরণ পরিমাপক এর সাহায্যে অবস্থান হালনাগাদ, আবহাওয়া বা অবস্থান অনুসরণ করতে আপনাকে ব্যাটারি অপ্টিমাইজেশন বন্ধ করতে বলবে।</string>
    <string name="alertDialog_battery_optimization_proceed" tools:ignore="MissingTranslation">চলা</string>
    <string name="alertDialog_battery_optimization_cancel" tools:ignore="MissingTranslation">বাতিল</string>
    <string name="alertDialog_location_permission_title" tools:ignore="MissingTranslation">অবস্থান এবং অনুমতি সেটিংস</string>
    <string name="alertDialog_location_permission_message_location_phone_settings" tools:ignore="MissingTranslation">অ্যাপ্লিকেশনটি অবস্থান আপডেট করার জন্য সেট করা আছে। এটি অ্যাক্সেস প্রয়োজন। ডিভাইস সেটিংসে যান এবং অবস্থানটি স্যুইচ করুন বা অ্যাপ্লিকেশন অবস্থান আপডেটগুলি বন্ধ করুন।</string>
    <string name="alertDialog_location_permission_message_location_gps_permission" tools:ignore="MissingTranslation">অ্যাপ্লিকেশনটির জিপিএস ব্যবহার করে অবস্থান আপডেট করার অনুমতি প্রয়োজন।</string>
    <string name="alertDialog_location_permission_message_location_network_permission" tools:ignore="MissingTranslation">অ্যাপ্লিকেশনটির ওয়াই ফাই এবং জিএসএম নেটওয়ার্কগুলির দ্বারা অবস্থান আপডেট করার জন্য একটি অনুমতি প্রয়োজন।</string>
    <string name="alertDialog_location_permission_message_location_phone_permission" tools:ignore="MissingTranslation">ওয়াই-ফাই এবং জিএসএম নেটওয়ার্কগুলি ব্যবহার করে অবস্থান আপডেট করার জন্য ডিভাইস থেকে তথ্য পড়ার অনুমতি প্রয়োজন। ডিভাইসটি কীসের সাথে সংযুক্ত রয়েছে তা জিএসএম বেস স্টেশনগুলি সম্পর্কে এটি তথ্য এনে দেয়।</string>
    <string name="alertDialog_location_permission_positiveButton_settings" tools:ignore="MissingTranslation">যন্ত্র সেটিংস</string>
    <string name="alertDialog_location_permission_positiveButton_permissions" tools:ignore="MissingTranslation">অনুমতি অনুরোধ</string>
</resources>
